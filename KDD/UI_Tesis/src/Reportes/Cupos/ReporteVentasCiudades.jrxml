		<![CDATA[SELECT cd.nombre_ciudad, dv.sede_afiliacion, cd.departamento_ciudad, SUM(dv.cantidad_afiliaciones) AS cantidad_afiliaciones FROM datamart_afiliacion dv INNER JOIN dim_ciudad cd  ON (dv.ciudad_afiliacion = CAST ( cd.cod_ciudad AS BIGINT )) WHERE dv.sede_afiliacion = $P{id_sede} AND cd.departamento_ciudad = CAST ($P{id_departamento} AS VARCHAR) AND (dv.fecha_afiliacion BETWEEN $P{id_fecha_inicio} AND $P{id_fecha_fin}) GROUP BY cd.nombre_ciudad, dv.sede_afiliacion, cd.departamento_ciudad ORDER BY SUM(dv.cantidad_afiliaciones) DESC LIMIT 5;]]>
	</queryString>
